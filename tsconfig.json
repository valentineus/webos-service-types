{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "moduleResolution": "Node",
    "newLine": "lf",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "strict": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "dist/**/*"
  ]
}
